<div class="events">
	<h2 style="text-align: center;">Events</h2>
	<% if @events.any? %>
		<% @events.each do |event| %>
			<h5><%= link_to event.description, event_path(event), style:'color:white; text-decoration: none;' %></h5>
		<%end%>
	<% else %>
		<h5>No Events Found </h5>
	<%end%>
</div>